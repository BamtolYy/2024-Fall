clear all
clc
%% Problem set 2

% 1

syms f
min = -2;
max = 2;
z=double(int(sinc(f)^2,min,max));

% delt =0.01;
% f = min:delt:max;
% Rx = 0;
% for i = 1:length(f)
%    Rx = Rx+delt*sinc(f(i))^2;
% end

delt =0.001;
f = min:delt:max;
Rx = 0;
for i = 2:length(f)
    if isnan(sin(pi*f(i))/(pi*f(i)))
        Rx = Rx+delt;
        graph(i)=Rx;
    else
        Rx = Rx +delt*(sin(pi*f(i))/(pi*f(i)))^2;
        graph(i)=Rx;
    end
end
fprintf('\n');
fprintf('%.3f%%',Rx*100)
fprintf('\n');

